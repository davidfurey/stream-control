{
  "name": "stream-control",
  "version": "0.1.3",
  "description": "Service to allow automatic and manual control of Youtube live stream",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:server": "webpack --config webpack.config.js --config-name server",
    "build:client": "webpack --config webpack.config.js --config-name client",
    "build:server:prod": "webpack --config webpack.config.js --config-name server --env.production",
    "build:client:prod": "webpack --config webpack.config.js --config-name client --env.production",
    "build:prod": "npm run build:server:prod; npm run build:client:prod",
    "watch:server": "webpack --config webpack.config.js --config-name server --env.watch",
    "watch:client": "webpack-dev-server --config webpack.config.js --config-name client",
    "watch": "npm run watch:server & npm run watch:client",
    "lint": "node_modules/eslint/bin/eslint.js -c .eslintrc.js ./ --ext '.ts,.tsx'",
    "lint:fix": "node_modules/eslint/bin/eslint.js -c .eslintrc.js ./ --ext '.ts,.tsx' --fix",
    "package": "npm run build:prod; cd dist; node-deb --install-strategy copy -- server.js auth-cli.js public"
  },
  "node_deb": {
    "dependencies": "npm",
    "entrypoints": {
      "daemon": "server.js",
      "cli": "auth-cli.js"
    },
    "executable_name": "stream-control-auth",
    "user": "stream-control",
    "group": "stream-control",
    "templates": {
      "default_variables": "../config/default.template"
    }
  },
  "keywords": [],
  "author": "David Furey",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.2",
    "eslint": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "html-webpack-plugin": "^4.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-deb": "^0.10.7",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^2.5.2",
    "webpack-permissions-plugin": "^1.0.7"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "@types/node-schedule": "^1.3.0",
    "array-flat-polyfill": "^1.0.1",
    "bootswatch": "^4.4.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "google-auth-library": "^6.0.6",
    "googleapis": "^59.0.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "node-fetch": "^2.6.1",
    "node-schedule": "^1.3.2",
    "obs-websocket-js": "^4.0.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  }
}
